package recapDemo1;

public class Main {

	public static void main(String[] args) {
		int sayi1=24;
		int sayi2=20;
		int sayi3=7;
		int enBuyuk=sayi1;
		if (enBuyuk<sayi2) {
			System.out.println("En büyük sayı: "+sayi2);
		}
		if (enBuyuk<sayi3) {
			System.out.println("En büyük sayı: "+sayi3);
		}
	}

}
