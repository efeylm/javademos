package arkadasSayilar;

public class Main {

	public static void main(String[] args) {
		// 220yi bölen sayıların toplamı 284 , 284ü bölen sayıların toplamı 220dir (kendileri hariç).
		// bu sayılar arkadaş sayılardır.
		int sayi1=220;
		int sayi2=284; 	
		int toplam=0;
		
		for (int i = 0; i < args.length; i++) {
			if (sayi1%i==0) {
				toplam=toplam+i;
			}
		}

	}

}
